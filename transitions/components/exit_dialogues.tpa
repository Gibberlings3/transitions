////////////////////////////
// PREP THE INTERJECTIONS //
////////////////////////////

/////////////////////////////////////////////////////
// Interjections
// Everyone in the group chimes in
// So we'll loop through all NPCs twice
// Once to set up the interjections, once to build the dialogue and baf files
// Building each line every time will make the install go slower
// However it'll help ensure each is done properly
// Each line of dialogue gets put into a variable
/////////////////////////////////////////////////////
//OUTER_FOR ( npc_num = 0; npc_num < max_num; ++npc_num ) BEGIN
OUTER_FOR (npc_num = 0; VARIABLE_IS_SET $npc_list(~%npc_num%~); ++npc_num) BEGIN 
   OUTER_SPRINT npc_name $npc_list("%npc_num%")
   OUTER_SPRINT upper_name "%npc_name%"
   ACTION_TO_UPPER upper_name
   OUTER_SPRINT join_file EVAL EVAL ~%%upper_name%_JOINED%~ 

   // This is the normal dialogue trigger for group interjections
   OUTER_SPRINT npc_ok ~~~~~== %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0)~ ~~~~~

   // This is the normal DO after the group's "leaving" dialogues 
   OUTER_SPRINT exit_do ~~~~~DO ~SetGlobal("#L_%npc_name%OkInBG1Areas","GLOBAL",1) ActionOverride("%upper_name%",LeaveParty()) ActionOverride("%upper_name%",EscapeArea())~ ~~~~~

   // NOTE: Only the first 7 do the post_Korlasz (PK) dialogues 
   ACTION_MATCH %npc_num% WITH
      0 BEGIN     // AJANTIS
         // Initial PK: Me as well.
         OUTER_SPRINT pk_i0 ~%npc_ok% @2340~

         // Staying PK: As you wish.  I owe you that at least.
         OUTER_SPRINT pk_s0 ~%npc_ok% @2283~

         // Leaving PK: It has been an honor.  Farewell.
         OUTER_SPRINT pk_l0 ~%npc_ok% @2284 %exit_do%~

         // Initial pre-SoD/BG2: I too will be leaving.
         OUTER_SPRINT exit_i0 ~%npc_ok% @2329~

         // Leaving pre-SoD/BG2: It has been an honor.  Farewell.
         OUTER_SPRINT exit_l0 ~%npc_ok% @2284 %exit_do%~
      END
      1 BEGIN     // JAHEIRA
         // Initial: I too have other places to be.
         OUTER_SPRINT pk_i1 ~%npc_ok% @2326~

         // Staying: So be it.
         OUTER_SPRINT pk_s1 ~%npc_ok% @2327~

         // Leaving: Take care of yourself.
         OUTER_SPRINT pk_l1 ~%npc_ok% @2328 %exit_do%~

         // Initial pre-SoD: I must go as well
         OUTER_SPRINT exit_i1 ~%npc_ok% @2335~

         // Leaving pre-SoD: Take care of yourself.
         OUTER_SPRINT exit_l1 ~%npc_ok% @2328 %exit_do%~
      END
      2 BEGIN     // KHALID
         // Khalid corrects Jaheira if they're both in the group
         // Initial: I-I should be going as well.
         OUTER_SPRINT pk_i2 ~~~~~== %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("JAHEIRA") Global("#L_JaheriaModded","GLOBAL",0)~ @2338 /* ~Yes, *we* should go.~ */ 
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) OR(2) !IsValidForPartyDialogue("JAHEIRA") !Global("#L_JaheriaModded","GLOBAL",0)~ @2332~~~~~

         // Staying: If you insist.
         OUTER_SPRINT pk_s2 ~%npc_ok% @2333~

         // Leaving: Goodbye.
         OUTER_SPRINT pk_l2 ~%npc_ok% @2334 %exit_do%~

         // Initial pre-SoD: I must go as well
         OUTER_SPRINT exit_i2 ~~~~~== %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("JAHEIRA") Global("#L_JaheriaModded","GLOBAL",0)~ @2339 /* ~Yes, *we* really must go.~ */ 
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) OR(2) !IsValidForPartyDialogue("JAHEIRA") !Global("#L_JaheriaModded","GLOBAL",0)~ @2335~~~~~
         
         // Leaving pre-SoD: Goodbye
         OUTER_SPRINT exit_l2 ~%npc_ok% @2334 %exit_do%~
      END
      3 BEGIN     // DORN
         // Initial: Me as well.
         OUTER_SPRINT pk_i3 ~%npc_ok% @2340~

         // Staying: I can stay for a little while.
         OUTER_SPRINT pk_s3 ~%npc_ok% @2341~

         // Leaving: Until we meet again.
         OUTER_SPRINT pk_l3 ~%npc_ok% @2346 %exit_do%~

         // Initial pre-SoD/BG2: Time to go!
         OUTER_SPRINT exit_i3 ~%npc_ok% @2343~
         
         // Leaving pre-SoD/BG2:  Until we meet again.
         OUTER_SPRINT exit_l3 ~%npc_ok% @2346 %exit_do%~
      END
      4 BEGIN     // EDWIN
