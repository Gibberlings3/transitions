         OUTER_SPRINT exit_l8 ~%npc_ok% @2374 %exit_do%~
      END
      9 BEGIN     // DYNAHEIR
         // Initial pre-SoD/BG2: Aye, a rest will do us all good.
         OUTER_SPRINT exit_i9 ~%npc_ok% @2381~

