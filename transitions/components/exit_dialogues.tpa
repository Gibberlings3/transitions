      22 BEGIN     // SAFANA
         // Initial pre-SoD/BG2: Yes, enough is enough.
         OUTER_SPRINT exit_i22 ~%npc_ok% @2423~
         
         // Leaving pre-SoD/BG2: Goodbye.  It's been...interesting.
         OUTER_SPRINT exit_l22 ~%npc_ok% @2424 %exit_do%~
