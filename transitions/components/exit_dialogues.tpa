      END
      19 BEGIN     // KIVAN
         // Initial pre-SoD/BG2: It is time to part ways.
         OUTER_SPRINT exit_i19 ~%npc_ok% @2414~
         
         // Leaving pre-SoD/BG2: Farewell, mellonamin.
         OUTER_SPRINT exit_l19 ~%npc_ok% @2415 %exit_do%~
