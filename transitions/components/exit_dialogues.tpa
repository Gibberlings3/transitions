         // There's a banter between Dynaheir and Minsc, so we make sure he's not in the group to do her exit
         OUTER_SPRINT exit_l9 ~~~~~== %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) OR(2)!IsValidForPartyDialogue("MINSC") GlobalGT("#L_MinscModded","GLOBAL",0)~ @2382 %exit_do% ~~~~~
      END
      10 BEGIN     // MINSC
         // Initial pre-SoD/BG2: Boo is weary and must rest!
         OUTER_SPRINT exit_i10 ~%npc_ok% @2376~

         // Leaving pre-SoD/BG2: If you need us, look for us at the Three Old Kegs.
         OUTER_SPRINT exit_l10 ~~~~~== DYNAHJ IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2382
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) OR(2) !IsValidForPartyDialogue("DYNAHEIR") GlobalGT("#L_DynaheirModded","GLOBAL",0)~ @2377 %exit_do%
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2377
      == DYNAHJ IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2384 /* ~I just said that.~ */
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2385 /* ~Oh, sorry, I didn't hear you.  I was talking to Boo.~ */
      == %join_file% IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2386 /* ~He said to make sure to pick up some fresh pumpkin seeds.~ */ %exit_do%
      == DYNAHJ IF ~IsValidForPartyDialogue("%upper_name%") Global("#L_%npc_name%Modded","GLOBAL",0) IsValidForPartyDialogue("DYNAHEIR") Global("#L_DynaheirModded","GLOBAL",0)~ @2387 DO ~SetGlobal("#L_DynaheirOkInBG1Areas","GLOBAL",1) ActionOverride("Dynaheir",LeaveParty()) ActionOverride("Dynaheir",EscapeArea())~ /* ~sigh~ */~~~~~
      END
      11 BEGIN     // ALORA
         // Initial pre-SoD/BG2: Yeah, think I'll check out...mmm somewhere..not here.
         OUTER_SPRINT exit_i11 ~%npc_ok% @2390~

         // Leaving pre-SoD/BG2: See ya 'round!
         OUTER_SPRINT exit_l11 ~%npc_ok% @2391 %exit_do%~
      END
      12 BEGIN     // BRANWEN
         // Initial pre-SoD/BG2: Aye, our time together is at an end.
         OUTER_SPRINT exit_i12 ~%npc_ok% @2393~

         // Leaving pre-SoD/BG2: Tempus watch over you!
         OUTER_SPRINT exit_l12 ~%npc_ok% @2394 %exit_do%~
      END
      13 BEGIN     // CORAN
         // Initial pre-SoD/BG2: Yes, it is time we part ways.
         OUTER_SPRINT exit_i13 ~%npc_ok% @2396~

         // Leaving pre-SoD/BG2: Farewell my friend.
         OUTER_SPRINT exit_l13 ~%npc_ok% @2397 %exit_do%~
      END
      14 BEGIN     // ELDOTH
         // Initial pre-SoD/BG2: It's about time we all came to our senses!  I'll be leaving too.
         OUTER_SPRINT exit_i14 ~%npc_ok% @2399~

         // Leaving pre-SoD/BG2: Ta ta!
         OUTER_SPRINT exit_l14 ~%npc_ok% @2400 %exit_do%~
