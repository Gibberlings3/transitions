         OUTER_SPRINT exit_l6 ~%npc_ok% @2362 %exit_do%~
      END
      7 BEGIN     // GARRICK
         // Initial pre-SoD/BG2: I can't wait any longer, either.
         OUTER_SPRINT exit_i7 ~%npc_ok% @2367~

