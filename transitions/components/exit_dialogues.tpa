         OUTER_SPRINT exit_l7 ~%npc_ok% @2368 %exit_do%~
      END
      8 BEGIN     // XAN
         // Initial pre-SoD/BG2: I need to report back to Evereska.
         OUTER_SPRINT exit_i8 ~%npc_ok% @2373~

