         OUTER_SPRINT pk_s5 ~%npc_ok% @2355~

         // Leaving: See ya!  Good luck and all.
         OUTER_SPRINT pk_l5 ~%npc_ok% @2356 %exit_do%~

         // Initial pre-SoD/BG2: Yes, I really do have to go.
         OUTER_SPRINT exit_i5 ~%npc_ok% @2357~

         // Leaving pre-SoD/BG2: See ya!  Good luck and all.
         OUTER_SPRINT exit_l5 ~%npc_ok% @2356 %exit_do%~
      END
      6 BEGIN     // BAELOTH
         // Initial: You hog all the limelight.
         OUTER_SPRINT pk_i6 ~%npc_ok% @2360~

         // Staying: Have it your way, as always!
         OUTER_SPRINT pk_s6 ~%npc_ok% @2361~

         // Leaving: Look for my name in lights, I'm bowing out of this dump!
         OUTER_SPRINT pk_l6 ~%npc_ok% @2362 %exit_do%~

